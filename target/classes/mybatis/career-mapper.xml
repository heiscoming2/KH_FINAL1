<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="career">

	<select id="selectOne" resultType="CareerDto" parameterType="int">
		SELECT *
		FROM CAREER
		WHERE M_NO = #{m_no}
	</select>
	
</mapper>